<?xml version="1.0" encoding="windows-1252"?>
<GeochemXYDiagram name="Cr2O3 vs MgO for kimberlitic ilmenites">
   <GeochemFunctionAxisX name="MgO (pct)" function="mg" log="false"/>
   <GeochemFunctionAxisY name="Cr2O3 (pct)" function="cr" log="false"/>
   <Comment>Kimberlitic ilmenite discrimination diagram (between 5 and 10wt% MgO)</Comment>
   <Reference>Diagram after Skinner et al., Lithos 76 (2004) 233-259</Reference>
   <Variable letter="CR" element="Cr2O3" unit="pct"/>
   <Variable letter="MG" element="MgO" unit="pct"/>
   <Poly name="" visible="true" closed="false">
      <Colour r="0" g="0" b="255"/>
      <LabelAngle angle="0.0" />
      <Boundary3>
         <Linear>
            <BezierPoint x="10.0" y="0.0" sectionEnd="true"/>
         </Linear>
         <Linear>
            <BezierPoint x="10.0" y="10.0" sectionEnd="true"/>
         </Linear>
      </Boundary3>
   </Poly>
   <Poly name="" visible="true" closed="false">
      <Colour r="0" g="0" b="255"/>
      <LabelAngle angle="0.0" />
      <Boundary3>
         <Linear>
            <BezierPoint x="5.0" y="0.0" sectionEnd="true"/>
         </Linear>
         <Linear>
            <BezierPoint x="5.0" y="10.0" sectionEnd="true"/>
         </Linear>
      </Boundary3>
   </Poly>
   <Polygon name="_ v6.1 required to open diagram _" visible="true" >
      <Point x="30" y="30" />
      <Point x="200" y="30" />
      <Point x="200" y="40" />
      <Point x="30" y="40" />
   </Polygon>
</GeochemXYDiagram>
