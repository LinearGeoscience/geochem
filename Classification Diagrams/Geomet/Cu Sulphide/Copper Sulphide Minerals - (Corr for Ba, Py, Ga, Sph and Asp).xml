   <Variable letter="A" element="Cu-mol" unit="mol/kg"/>
   <Variable letter="B" element="Fe-mol" unit="mol/kg"/>
   <Variable letter="C" element="S-mol" unit="mol/kg"/>
   <Variable letter="D" element="Ba-mol" unit="mol/kg"/>
   <Variable letter="E" element="Zn-mol" unit="mol/kg"/>
   <Variable letter="F" element="As-mol" unit="mol/kg"/>
   <Variable letter="G" element="Pb-mol" unit="mol/kg"/>
   <Line name="Cubanite" visible="true" m="0.33" c="0.0" >
